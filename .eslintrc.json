{
  "extends": "next/core-web-vitals",
  "rules": {
    "indent": ["error", 2],
    "semi": [2, "always"],
    "comma-dangle": ["error", "always-multiline"],
    "quotes": [2, "single", { "avoidEscape": true }],
    "object-curly-spacing": [2, "always"],
    "jsx-quotes": ["error", "prefer-double"]
  }
}
